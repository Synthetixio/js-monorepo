{
  "name": "@snx-v3/spot-markets-ui",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "webpack-cli serve",
    "build": "NODE_ENV=production webpack-cli",
    "typecheck": "tsc --noEmit",
    "standalone-install": "yarn workspaces focus '@synthetixio/v3-ui'",
    "standalone-build": "yarn workspaces foreach --topological-dev --recursive --verbose --from '@synthetixio/v3-ui' run build",
    "deploy": "ipfs-deploy ./dist"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.0.17",
    "@chakra-ui/react": "^2.5.0",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@snx-v3/constants": "workspace:*",
    "@snx-v3/format": "workspace:*",
    "@snx-v3/icons": "workspace:*",
    "@snx-v3/useBlockchain": "workspace:*",
    "@snx-v3/useGasSpeed": "workspace:*",
    "@synthetixio/v3-theme": "workspace:*",
    "@tanstack/react-query-devtools": "^4.24.6",
    "framer-motion": "^7.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^6.8.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/runtime-corejs3": "^7.20.13",
    "@noisekit/ipfs-deploy": "^1.0.3",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
    "@tanstack/react-query": "^4.24.6",
    "@types/react": "^18.0.28",
    "@types/react-helmet": "^6.1.6",
    "babel-loader": "^9.1.2",
    "bn.js": "^5.2.1",
    "core-js": "3",
    "crypto-browserify": "^3.12.0",
    "css-loader": "^6.7.3",
    "html-webpack-plugin": "^5.5.0",
    "react-refresh": "^0.14.0",
    "stream-browserify": "^3.0.0",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.6",
    "typescript": "^4.9.5",
    "webpack": "^5.75.0",
    "webpack-bundle-analyzer": "^4.8.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  },
  "browserslist": [
    "last 1 Chrome version",
    "last 1 iOS version",
    "last 1 Safari version",
    "last 1 Firefox version",
    "last 1 Edge version",
    "last 1 Opera version"
  ],
  "depcheck": {
    "ignorePatterns": [
      "dist"
    ],
    "ignoreMatches": [
      "@noisekit/ipfs-deploy"
    ]
  }
}

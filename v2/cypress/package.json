{
  "name": "@snx-v2/cypress",
  "private": true,
  "main": "index.ts",
  "version": "0.0.0",
  "scripts": {
    "start": "yarn cy",
    "cy": "cypress open --component --browser chrome",
    "cy:check": "cypress run --component",
    "e2e": "cypress open --e2e --browser chrome",
    "e2e:check": "cypress run --e2e"
  },
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@chakra-ui/react": "^2.2.8",
    "babel-loader": "^8.2.5",
    "babel-plugin-module-resolver": "^4.1.0",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.0",
    "cypress": "^10.9.0",
    "ethers": "^5.5.3",
    "process": "^0.11.10",
    "react-router-dom": "^6.3.0",
    "stream-browserify": "^3.0.0",
    "webpack": "^5.74.0"
  },
  "depcheck": {
    "ignoreMatches": [
      "assets",
      "components",
      "containers",
      "content",
      "contracts",
      "hoc",
      "hooks",
      "i18n",
      "lib",
      "mutations",
      "queries",
      "sections",
      "store",
      "styles",
      "tests",
      "translations",
      "utils",
      "buffer",
      "process"
    ]
  }
}

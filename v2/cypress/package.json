{
  "name": "@snx-v2/cypress",
  "private": true,
  "main": "index.ts",
  "version": "0.0.8",
  "scripts": {
    "start": "yarn cy",
    "cy": "cypress open --component --browser chrome",
    "cy:check": "cypress run --component --browser chrome",
    "e2e": "cypress open --e2e --browser chrome",
    "e2e:check": "cypress run --e2e --browser chrome"
  },
  "devDependencies": {
    "@chakra-ui/react": "^2.5.0",
    "@cypress/code-coverage": "^3.10.0",
    "@snx-cy/onLogAdded": "workspace:*",
    "@snx-cy/printBrowserLogs": "workspace:*",
    "@synthetixio/contracts": "workspace:*",
    "@synthetixio/v2-ui": "workspace:*",
    "cypress": "^12.5.1",
    "ethers": "^5.7.2",
    "react-router-dom": "^6.8.1"
  },
  "depcheck": {
    "ignoreMatches": [
      "assets",
      "components",
      "containers",
      "content",
      "contracts",
      "hoc",
      "hooks",
      "i18n",
      "lib",
      "mutations",
      "queries",
      "sections",
      "store",
      "styles",
      "tests",
      "translations",
      "utils"
    ]
  }
}
